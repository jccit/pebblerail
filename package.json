{
  "name": "pebblerail",
  "author": "jcit",
  "version": "0.8",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "scripts": {
    "build": "node resources/operators.mjs && pkts build"
  },
  "pebble": {
    "displayName": "PebbleRail",
    "uuid": "4385412a-fc54-4222-9c83-e392d4f22fa8",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite",
      "emery"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "jsReady",
      "request",
      "requestData",
      "objectType",
      "count",
      "serviceID",
      "locationName",
      "distance",
      "crs",
      "time",
      "platform",
      "operatorCode",
      "callingPointState",
      "isCancelled",
      "cancelReason",
      "delayReason",
      "origin",
      "destination"
    ],
    "resources": {
      "media": [
        {
          "menuIcon": true,
          "type": "png",
          "name": "APP_ICON",
          "file": "app_icon.png"
        },
        {
          "type": "raw",
          "name": "TRAIN_TINY",
          "file": "train_tiny.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_SMALL",
          "file": "train_small.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_LARGE",
          "file": "train_large.pdc"
        },
        {
          "type": "raw",
          "name": "METRO_TINY",
          "file": "metro_tiny.pdc"
        },
        {
          "type": "raw",
          "name": "METRO_SMALL",
          "file": "metro_small.pdc"
        },
        {
          "type": "png",
          "name": "DOWN_ARROW_BLACK",
          "file": "down_arrow_black.png"
        },
        {
          "type": "png",
          "name": "DOWN_ARROW_WHITE",
          "file": "down_arrow_white.png"
        },
        {
          "type": "png",
          "name": "UP_ARROW_BLACK",
          "file": "up_arrow_black.png"
        },
        {
          "type": "png",
          "name": "UP_ARROW_WHITE",
          "file": "up_arrow_white.png"
        },
        {
          "type": "raw",
          "name": "OPERATOR_INFO",
          "file": "operators.bin"
        }
      ],
      "publishedMedia": [
        {
          "name": "TRAIN",
          "id": 1,
          "timeline": {
            "tiny": "TRAIN_TINY",
            "small": "TRAIN_SMALL",
            "large": "TRAIN_LARGE"
          }
        }
      ]
    },
    "capabilities": [
      "location"
    ]
  },
  "dependencies": {
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "pkts": "^0.2.1"
  }
}
