{
  "name": "pebblerail",
  "author": "jcit",
  "version": "0.3.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "scripts": {
    "build": "yarn proto && rollup -c",
    "proto": "pbjs -t static-module -w es6 --es6 -o src/ts/gen/stations.js src/ts/proto/stations.proto && pbts -o src/ts/gen/stations.d.ts src/ts/gen/stations.js"
  },
  "pebble": {
    "displayName": "PebbleRail",
    "uuid": "4385412a-fc54-4222-9c83-e392d4f22fa8",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "jsReady",
      "request",
      "requestData",
      "objectType",
      "count",
      "serviceID",
      "locationName",
      "distance",
      "crs",
      "time",
      "platform",
      "operatorCode",
      "callingPointState",
      "isCancelled",
      "cancelReason",
      "delayReason",
      "origin",
      "destination"
    ],
    "resources": {
      "media": [
        {
          "menuIcon": true,
          "type": "png",
          "name": "APP_ICON",
          "file": "app_icon.png"
        },
        {
          "type": "raw",
          "name": "TRAIN_TINY",
          "file": "train_tiny.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_SMALL",
          "file": "train_small.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_LARGE",
          "file": "train_large.pdc"
        },
        {
          "type": "png",
          "name": "DOWN_ARROW_BLACK",
          "file": "down_arrow_black.png"
        },
        {
          "type": "png",
          "name": "DOWN_ARROW_WHITE",
          "file": "down_arrow_white.png"
        },
        {
          "type": "png",
          "name": "UP_ARROW_BLACK",
          "file": "up_arrow_black.png"
        },
        {
          "type": "png",
          "name": "UP_ARROW_WHITE",
          "file": "up_arrow_white.png"
        }
      ],
      "publishedMedia": [
        {
          "name": "TRAIN",
          "id": 1,
          "timeline": {
            "tiny": "TRAIN_TINY",
            "small": "TRAIN_SMALL",
            "large": "TRAIN_LARGE"
          }
        }
      ]
    },
    "capabilities": [
      "location"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@babel/runtime": "^7.26.7",
    "@babel/runtime-corejs3": "^7.26.7",
    "core-js": "^3.40.0",
    "protobufjs": "^7.4.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "protobufjs-cli": "^1.1.3",
    "rollup": "^4.34.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  }
}
